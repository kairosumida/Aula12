@model IEnumerable<Aula12.Models.Autenticacao>

@{
    ViewBag.Title = "Index";
}

<h2>Listar</h2>

<p>
    @Html.ActionLink("Criar Nova", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Usuario.Nome)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Data)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Usuario.Nome)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Data)
        </td>
        <td>
            @Html.ActionLink("Editar", "Edit", new { id=item.AutenticacaoId }) |
            @Html.ActionLink("Detalhes", "Details", new { id=item.AutenticacaoId }) |
            @Html.ActionLink("Deletar", "Delete", new { id=item.AutenticacaoId })
        </td>
    </tr>
}

</table>
